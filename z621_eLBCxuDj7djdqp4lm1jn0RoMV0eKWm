    const createSlot = (slotName, elementID, adUnitID, dimensions, refresh = false) => {
        window[slotName] = window.ANAWeb.createSlot(SlotTypeEnum.GOOGLE, {
            elementID,
            adUnitID,
            dimensions,
            collapseBeforeFill: true,
