    document.getElementsByClassName("ad-block-banner-container")[0].children[0].id = 'slot-banner';

    if (window.innerWidth > 1490) {
        if (!window.isWanaLoaded) {
            new Promise(async (resolve, reject) => {
                try {
                    await window.wanaScript()

                    if (window.slotBanner) {
                        throw new Error('Slot already exists');
                    }

                    window.createSlot('slotBanner', 'slot-banner', '/114106652/amino_728x90_atf_desktop_1', [[728, 90]])

                    resolve(window[this.global])
                } catch (e) {
                    reject(e)
                }
            })
        } else if (!window.slotBanner) {
            window.createSlot('slotBanner', 'slot-banner', '/114106652/amino_728x90_atf_desktop_1', [[728, 90]])
        }
    } 
</script> <article class="post main-post hide-blocker " >   <header class="title">  <h1 class="title community-color force-word-break" data-vce="rich-content"> Женщина-Халк: #1 </h1> </header> <section class="info">  <a href="https://aminoapps.com/c/vselennaia-marvel-dc/page/user/deadpool/2vg0_YXps6fm3GBExVr6Mkv2RGMDvbDrVjP" class="block"> <img class="author avatar img-cover" alt="Author's Avatar" data-vce="image"  src="//pm1.narvii.com/7680/5288fc518de51a3936ac8a42b029dccba469b9b8r1-736-851v2_128.jpg"  > </a> <div class="overflow-hidden"> <a class="nickname" href="https://aminoapps.com/c/vselennaia-marvel-dc/page/user/deadpool/2vg0_YXps6fm3GBExVr6Mkv2RGMDvbDrVjP" data-vce="rich-content">  <span class="content">~• :boom: 🌮DEADPOOL🌮 :boom: •~</span>  <i class="level-badge level-badge-9"></i>  </a> <span class="pretty-date"> 06/08/18 </span> </div>   <section class="counters">  <div class="vote-counter pointer" data-vce="btn-like" data-vote-value="0" data-href="https://aminoapps.com/c/vselennaia-marvel-dc/page/blog/zhenshchina-khalk-1/z621_eLBCxuDj7djdqp4lm1jn0RoMV0eKWm"   data-target-ndc-id="176535932" data-target-post-type="blog" data-target-post-id="94ea451c-4630-4e71-8189-35621d446d73" data-target-log-type="image"  data-join-icons="" data-join-deeplink="narviiapp://x176535932/blog/94ea451c-4630-4e71-8189-35621d446d73" data-join-motivation="like" data-join-motivation-like="page" data-join-copytext="https://aminoapps.com/c/vselennaia-marvel-dc/page/blog/zhenshchina-khalk-1/z621_eLBCxuDj7djdqp4lm1jn0RoMV0eKWm" data-login-motivation="Sign in to like this post." data-event-label="like_post" data-submit-event-label="live_comment" data-source="Like Post" > <i class="amino-icon amino-icon-heart-o"></i> <span class="num">8</span> </div>  <div class="comment-counter pointer" data-vce="btn-popup" data-popup="join-community" data-popup-icons="//pm1.narvii.com/7680/5288fc518de51a3936ac8a42b029dccba469b9b8r1-736-851v2_128.jpg" data-popup-deeplink="narviiapp://x176535932/blog/94