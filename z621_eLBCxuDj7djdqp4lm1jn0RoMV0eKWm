    const loadWana = () => {
        if (!window.isWanaLoaded) {
            new Promise(async (resolve, reject) => {
                try {

                    await window.wanaScript();

                    if (window.slotLeftAside) {
                        throw new Error('Slot already exists');
                    }

                    window.createSlot('slotLeftAside', 'slot-left-aside', '/114106652/amino_300x250_atf_desktop_2', [[300, 250]])

                    resolve(window[this.global])
                } catch (e) {
                    reject(e)
                }
            })
        } else if (!window.slotLeftAside) {
            window.createSlot('slotLeftAside', 'slot-left-aside', '/114106652/amino_300x250_atf_desktop_2', [[300, 250]])
        }
    }

    loadWana();

    // Refresh ads every 30s
    setInterval(() => {
        if (true) {
            if (!window.isWanaLoaded) {
                loadWana()
            } else {
                window.slotLeftAside.refresh()
            }
        }
    }, 30000)
</script>  <section class="sidebar-card chatroom-sidebar" data-vce="sidebar-chat-list"> <h3 class="label community-color"> <i class="amino-icon amino-icon-reload j-reload"></i> Public Chatrooms </h3> <div class="chatroom-list-container simple-list"> </div> <div class="loading-overlay"> <i class="fa fa-spin fa-circle-o-notch community-color"></i> </div> </section> </div>  <div class="content" data-vce-name="sidebar_content" data-vce="sticky"></div>  </aside>   <aside class="community-aside recommendation-sidebar"> <div class="content " data-vce="page-aside">  <section class="author-aside j-followee-number-container hide-blocker" >  <div class="info"> <a href="https://aminoapps.com/c/vselennaia-marvel-dc/page/user/deadpool/2vg0_YXps6fm3GBExVr6Mkv2RGMDvbDrVjP" class="block" data-refer-type="sidebar_author">  <img src="//pm1.narvii.com/7680/5288fc518de51a3936ac8a42b029dccba469b9b8r1-736-851v2_128.jpg" class="avatar" />  </a> <div class="overflow-hidden"> <a class="block nickname" href="https://aminoapps.com/c/vselennaia-marvel-dc/page/user/deadpool/2vg0_YXps6fm3GBExVr6Mkv2RGMDvbDrVjP" data-vce="rich-content" data-refer-type="sidebar_author">  <span class="content">~• :boom: 🌮DEADPOOL🌮 :boom: •~</span>  </a>  <div class="pretty-date"> Member since Jun 2018 </div>  </div> </div> <div class="counters"> <div class="counter"> <div class="num">1618</div> <h5 class="label">Reputation</h5> </div> <div class="counter"> <div class="num">59</div> <h5 class="label">Following</h5> </div> <div class="counter"> <div class="num j-followee-number-target">134</div> <h5 class="label">  Followers  </h5> </div> </div>   <a class="follow-btn pointer block"  data-login-motivation="Sign in to follow ~•💥🌮DEADPOOL🌮💥•~." data-vce="pre-login-btn" data-true-form="user/follow-btn" data-join-icons="//pm1.narvii.com/7680/5288fc518de51a3936ac8a42b029dccba469b9b8r1-736-851v2_128.jpg" data-join-motivation="follow-pc" data-event-label="follow" data-source="Follow"  data-follow-event-label="author_card" da